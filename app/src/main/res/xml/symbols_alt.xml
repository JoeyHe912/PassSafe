<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyHeight="8%"
    android:keyWidth="10%p"
    android:verticalGap="0px">

    <Row android:verticalGap="-10px">
        <Key
            android:codes="-13"
            android:horizontalGap="8%"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/ic_safe" />
        <Key
            android:codes="-14"
            android:horizontalGap="8%"
            android:keyIcon="@drawable/ic_account" />
        <Key
            android:codes="-15"
            android:horizontalGap="8%"
            android:keyIcon="@drawable/ic_password" />
        <Key
            android:codes="-16"
            android:horizontalGap="8%"
            android:keyIcon="@drawable/ic_dice" />
        <Key
            android:codes="-17"
            android:horizontalGap="8%"
            android:keyIcon="@drawable/ic_save" />
    </Row>

    <Row>
        <Key
            android:keyEdgeFlags="left"
            android:keyLabel="~" />
        <Key android:keyLabel="`" />
        <Key android:keyLabel="|" />
        <Key android:keyLabel="•" />
        <Key android:keyLabel="√" />
        <Key android:keyLabel="π" />
        <Key android:keyLabel="÷" />
        <Key android:keyLabel="×" />
        <Key android:keyLabel="¶" />
        <Key
            android:keyEdgeFlags="right"
            android:keyLabel="∆" />
    </Row>

    <Row>
        <Key
            android:horizontalGap="5%"
            android:keyEdgeFlags="left"
            android:keyLabel="£" />
        <Key android:keyLabel="¢" />
        <Key android:keyLabel="€" />
        <Key android:keyLabel="¥" />
        <Key android:keyLabel="^" />
        <Key android:keyLabel="°" />
        <Key android:keyLabel="=" />
        <Key android:keyLabel="{" />
        <Key
            android:keyEdgeFlags="right"
            android:keyLabel="}" />
    </Row>

    <Row>
        <Key
            android:codes="-6"
            android:keyEdgeFlags="left"
            android:keyLabel="\?123"
            android:keyWidth='15%' />
        <Key android:keyLabel="\\" />
        <Key android:keyLabel='©"' />
        <Key android:keyLabel="®'" />
        <Key android:keyLabel="™" />
        <Key android:keyLabel="℅" />
        <Key android:keyLabel="[" />
        <Key android:keyLabel="]" />
        <Key
            android:codes="-5"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_keyboard_backspace"
            android:keyWidth='15%' />
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key
            android:codes="-2"
            android:keyEdgeFlags="left"
            android:keyLabel="ABC"
            android:keyWidth="15%" />
        <Key android:keyLabel="," />
        <Key android:keyLabel="&lt;" />
        <Key
            android:codes="32"
            android:isRepeatable="true"
            android:keyIcon="@drawable/ic_keyboard_space"
            android:keyWidth="30%p" />
        <Key android:keyLabel="&gt;" />
        <Key android:keyLabel="." />
        <Key
            android:codes="-4"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_keyboard_return"
            android:keyWidth="15%p" />
    </Row>
</Keyboard>

